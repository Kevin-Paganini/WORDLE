<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TabPane fx:id="TAB_PANE" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wordle.Controller">
   <tabs>
      <Tab closable="false" text="Wordle">
         <content>
            <AnchorPane fx:id="MAIN_PANE" prefHeight="400.0" prefWidth="600.0">

</AnchorPane>
         </content>
      </Tab>
       <Tab closable="false" text="Settings">
           <content>
               <Pane fx:id="SETTINGS_PANE">
               <children>
                   <Button fx:id="fiveLetterDictionary" layoutX="29.0" layoutY="24.0" mnemonicParsing="false" onAction="#fiveLetterWord" text="5 Letter Words" />
                   <Button fx:id="sixLetterDictionary" layoutX="149.0" layoutY="24.0" mnemonicParsing="false" onAction="#sixLetterWord" text="6 Letter Words" />
                   <Button fx:id="sevenLetterDictionary" layoutX="269.0" layoutY="24.0" mnemonicParsing="false" onAction="#sevenLetterWord" text="7 Letter Words" />
                   <Button fx:id="importDictionaryButton" layoutX="389.0" layoutY="24.0" mnemonicParsing="false" onAction="#importDictionary" text="Import Dictionary File" />
                  <Button fx:id="dark_light" layoutX="29.0" layoutY="72.0" mnemonicParsing="false" onAction="#dark_light_mode_switch" text="DARK-MODE" />
                   <Button fx:id="contrast" layoutX="29.0" layoutY="120.0" mnemonicParsing="false" onAction = "#contrast_switch" text="HIGH-CONTRAST-MODE" />
                   <TextField fx:id="numGuess" layoutX="29.0" layoutY="169.0" maxWidth="150" maxHeight="70" promptText="Number of Guesses" />
                   <Button fx:id="numChange" layoutX="190.0" layoutY="169.0" mnemonicParsing="false" onAction = "#changeGuessAmount" text="Submit" />
               </children></Pane>
           </content>
       </Tab>
   </tabs>
</TabPane>
